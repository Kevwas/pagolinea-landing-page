const deadline="July 29 2020 23:59:59 GMT-0300";function getTimeRemaining(s){const e=Date.parse(s)-Date.parse(new Date),i=Math.floor(e/1e3%60),n=Math.floor(e/1e3/60%60),d=Math.floor(e/36e5%24);return{total:e,days:Math.floor(e/864e5),hours:d,minutes:n,seconds:i}}let timer=setInterval(function(){const s=getTimeRemaining(deadline);document.getElementById("timer").innerHTML='<div class="days">       <div class="numbers">'+s.days+'</div>d√≠as</div> <div class="hours">   <div class="numbers">'+s.hours+'</div>horas</div> <div class="minutes">   <div class="numbers">'+s.minutes+'</div>minutos</div> <div class="seconds">   <div class="numbers">'+s.seconds+"</div>segundos</div> </div>"},1e3);